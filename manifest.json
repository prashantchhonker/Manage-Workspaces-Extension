{
    "name": "Webpage Thoughtspaces",
    "version": "1.0",
    "description": "Webpage Thoughtspaces",
    "permissions": [
        "tabs",
        "clipboardRead"
    ],
    "background":
    {},
    "browser_action":
    {
        "default_popup": "thoughtspaceList.html"
    },
    "web_accessible_resources": [
        "fonts/*.ttf"
    ],
    "icons":
    {
        "16": "icon-16.png",
        "32": "icon-32.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "manifest_version": 2
}